# Database Configuration
DATABASE_URL=mysql://username:password@localhost:3306/agent_platform

# Redis Configuration
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here

# Server Configuration
APP_SERVER_HOST=0.0.0.0
APP_SERVER_PORT=8080

# CORS Configuration
# Allow all localhost requests (true/false)
CORS_ALLOW_ALL_LOCALHOST=true
# Additional allowed origins (comma-separated)
# Example: CORS_ALLOWED_ORIGINS=https://example.com,https://app.example.com
CORS_ALLOWED_ORIGINS=

# Logging Configuration
APP_LOGGING_LEVEL=info

# OSS Configuration
OSS_ENDPOINT=oss-cn-beijing.aliyuncs.com
OSS_ACCESS_KEY_ID=your-access-key-id
OSS_ACCESS_KEY_SECRET=your-access-key-secret
OSS_BUCKET=your-bucket-name
OSS_UPLOAD_PATH=uploads
OSS_DOWNLOAD_DOMAIN=https://your-bucket-name.oss-cn-beijing.aliyuncs.com